en and the clicked target is not within the menu,\n      // then close the menu\n      if (menuOpened && ref.current && !ref.current.contains(e.target)) {\n        setMenuOpened(false)\n      }\n    }\n\n    document.addEventListener(\"mousedown\", checkIfClickedOutside)\n\n    return () => {\n      // Cleanup the event listener\n      document.removeEventListener(\"mousedown\", checkIfClickedOutside)\n    }\n  }, [menuOpened]) \n\n  const ref = useRef();  \n\n  function handleMenu() {\n    setMenuOpened(!menuOpened);\n  }\n\n\n  return (\n      <>\n    <div className=\"h-[56px] bg-[#353535] flex justify-center pr-[95px]\" ref={ref}>\n    <div className=\"container md:w-[1300px] w-[100vw] flex items-center justify-between\">\n      <nav className=\"flex gap-2 items-center\">\n      <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-4 w-4 text-white\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\" stroke-width=\"2\">\n        <path stroke-linecap=\"round\" stroke-linejoin=\"round\" d=\"M9.75 17L9 20l-1 1h8l-1-1-.75-3M3 13h18M5 17h14a2 2 0 002-2V5a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z\" />\n       </svg>\n       <a className=\"font-sans text-sm text-white mr-3 font-bold\" href=\"/interviews\">Interviews</a>\n       <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-4 w-4 text-white\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\" stroke-width=\"2\">\n        <path stroke-linecap=\"round\" stroke-linejoin=\"round\" d=\"M15.232 5.232l3.536 3.536m-2.036-5.036a2.5 2.5 0 113.536 3.536L6.5 21.036H3v-3.572L16.732 3.732z\" />\n       </svg>\n       <a className= \"font-sans text-sm text-white mr-3 font-bold\" href=\"/contribute\">Contribute</a>\n      </nav>\n      <a href=\"/\">\n       <img src={Logo} alt=\"logo\" width=\"150\" />\n      </a>\n      <div className=\"flex gap-3 items-center\">\n        <span className=\"text-white font-semibold text-xs\">@cmartdec</span>\n        <button onClick={handleMenu}>\n          <img className=\"w-[3